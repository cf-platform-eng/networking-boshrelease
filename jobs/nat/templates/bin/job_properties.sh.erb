#!/usr/bin/env bash

#
# NAT job properties
#

# Directory to store the NAT job configuration files
export NAT_CONF_DIR=${JOB_DIR}/config

# Directory to store the NAT job log files
export NAT_LOG_DIR=${LOG_DIR}

# Directory to store the NAT job process IDs
export NAT_PID_DIR=${RUN_DIR}

# Directory to store the NAT job data files
export NAT_STORE_DIR=${STORE_DIR}

# Directory to store the NAT job temp files
export NAT_TMP_DIR=${TMP_DIR}
